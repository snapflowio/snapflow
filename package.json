  "license": "AGPL-3.0-only",
  "private": true,
  "scripts": {
    "build": "nx run-many --target=build --all --parallel=$(getconf _NPROCESSORS_ONLN) --configuration=development",
    "build:production": "nx run-many --target=build --all --parallel=$(getconf _NPROCESSORS_ONLN) --configuration=production",
    "serve": "nx run-many --target=serve --all --exclude=daemon --parallel=$(getconf _NPROCESSORS_ONLN) --configuration=development",
    "serve:production": "nx run-many --target=serve --all --exclude=daemon --parallel=$(getconf _NPROCESSORS_ONLN) --configuration=production",
    "generate:openapi": "nx run-many --target=openapi --all",
    "generate:api-client": "bun run generate:openapi && nx run-many --target=generate:api-client --all",
    "migration:generate": "cd apps/api && bunx drizzle-kit generate",
    "migration:run": "cd apps/api && bunx drizzle-kit push",
    "db:studio": "cd apps/api && bunx drizzle-kit studio",
    "shadcn:add": "TS_NODE_PROJECT=apps/dashboard/tsconfig.app.json bunx shadcn@latest add",
    "publish": "nx run-many --target=publish --all",
    "lint": "biome lint .",
    "lint:check": "biome lint --apply .",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
